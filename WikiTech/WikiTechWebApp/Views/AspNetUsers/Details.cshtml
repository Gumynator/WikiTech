@model WikiTechAPI.Models.AspNetUsers

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <link rel="stylesheet" href="/css/profil.css">
</head>
<h1>Details</h1>

<div>
    <h4>AspNetUsers</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NomAspnetuser)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomAspnetuser)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PrenomAspnetuser)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PrenomAspnetuser)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.UserName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IsprivateAspnetuser)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IsprivateAspnetuser)
        </dd>     
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DatecreationAspnetuser)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DatecreationAspnetuser)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NbpointAspnetuser)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NbpointAspnetuser)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.LockoutEnd)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LockoutEnd)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdGenreNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdGenreNavigation.NomGenre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdGradeNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdGradeNavigation.NomGrade)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdVilleNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdVilleNavigation.NomVille)
        </dd>
    </dl>
</div>
<div class="page-content page-container" id="page-content">
    <div class="padding">
        <div class="row container d-flex justify-content-center">
            <div class="col-xl-6 col-md-12">
                <div class="card user-card-full">
                    <div class="row m-l-0 m-r-0">
                        <div class="col-sm-4 bg-c-lite-green user-profile">
                            <div class="card-block text-center text-white">
                                <div class="m-b-25"> <img src="/image/LogoWitkiTech.png" class="img-radius" alt="User-Profile-Image"> </div>
                                <h6 class="f-w-600">@Html.DisplayFor(model => model.PrenomAspnetuser) @Html.DisplayFor(model => model.NomAspnetuser)</h6>
                                <p>@Html.DisplayFor(model => model.IdGradeNavigation.NomGrade)</p> <i class=" mdi mdi-square-edit-outline feather icon-edit m-t-10 f-16"></i>
                            </div>
                        </div>
                        <div class="col-sm-8">
                            <div class="card-block">
                                <h6 class="m-b-20 p-b-5 b-b-default f-w-600">Information</h6>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p class="m-b-10 f-w-600">Nom d'utilisateur</p>
                                        <h6 class="text-muted f-w-400">@Html.DisplayFor(model => model.UserName)</h6>
                                    </div>
                                    <div class="col-sm-6">
                                        <p class="m-b-10 f-w-600">Genre</p>
                                        <h6 class="text-muted f-w-400">@Html.DisplayFor(model => model.IdGenreNavigation.NomGenre)</h6>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p class="m-b-10 f-w-600">Membre depuis</p>
                                        <h6 class="text-muted f-w-400">@Html.DisplayFor(model => model.DatecreationAspnetuser)</h6>
                                    </div>
                                </div>
                                <h6 class="m-b-20 m-t-40 p-b-5 b-b-default f-w-600">Projects</h6>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <p class="m-b-10 f-w-600">Recent</p>
                                        <h6 class="text-muted f-w-400">Sam Disuja</h6>
                                    </div>
                                    <div class="col-sm-6">
                                        <p class="m-b-10 f-w-600">Most Viewed</p>
                                        <h6 class="text-muted f-w-400">Dinoter husainm</h6>
                                    </div>
                                </div>
                                <ul class="social-link list-unstyled m-t-40 m-b-10">
                                    <li><a href="#!" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="facebook" data-abc="true"><i class="mdi mdi-facebook feather icon-facebook facebook" aria-hidden="true"></i></a></li>
                                    <li><a href="#!" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="twitter" data-abc="true"><i class="mdi mdi-twitter feather icon-twitter twitter" aria-hidden="true"></i></a></li>
                                    <li><a href="#!" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="instagram" data-abc="true"><i class="mdi mdi-instagram feather icon-instagram instagram" aria-hidden="true"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div>
    
    <a href="@ViewBag.returnUrl">Retour</a>
    
    
</div>
